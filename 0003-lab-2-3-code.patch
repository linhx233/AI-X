+		--inj-vnet=0 --synthetic=uniform_random \
+		--sim-cycles=$SIM_CYCLES --injectionrate=$INJ_RATE
+		INJ_TOT=$(grep -Eo "packets_injected::total\s*[0-9.]*" m5out/stats.txt | grep -Eo "[0-9.]*")
